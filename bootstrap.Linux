#!/bin/bash

SCRIPTDIR=$(cd `dirname $0` && pwd)
GOVERSION=1.11

# Create Links
cd ~
[[ ! -L .screenrc ]] && ln -s $SCRIPTDIR/screenrc .screenrc

# Install packages
sudo $SCRIPTDIR/bootstrap.apt

# Install go
sudo $SCRIPTDIR/bootstrap.go.Linux ${GOVERSION}

# Install ripgrep
sudo $SCRIPTDIR/bootstrap.ripgrep.Linux
